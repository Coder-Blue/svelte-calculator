{
  "productName": "Svelte Calculator",
  "version": "0.2.0",
  "identifier": "com.svelte-calculator.noah",
  "build": {
    "frontendDist": "../build",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "bun run dev",
    "beforeBuildCommand": "bun run build"
  },
  "app": {
    "windows": [
      {
        "title": "Máy tính Svelte",
        "width": 455,
        "height": 690,
        "transparent": true,
        "decorations": false,
        "resizable": false,
        "fullscreen": false,
        "shadow": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "$schema": "../node_modules/@tauri-apps/cli/schema.json"
}
